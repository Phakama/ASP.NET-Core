@model IEnumerable<ApplicationUser>

<h2>List of Users</h2>
<hr />
@if (!Model.Any())
{
    <p>No rcords available on the users table!</p>

}
else
{


<table class="table table-striped table-bordered" id="userlist">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Cell Number</th>
            <th>Email Address</th>
            <th>Edit Employee</th>
        </tr>
    </thead>
    <tbody id="usertable">
       @foreach (var user in Model)
       {
        <tr>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.CellNumber</td>
            <td>@user.EmailAddress</td>
            <td>
                <div class="form-group">
                    <input type="submit" class="btn btn-primary edit" value="Edit" />
                </div>
            </td>
        </tr>

       }
    </tbody>
</table>
}